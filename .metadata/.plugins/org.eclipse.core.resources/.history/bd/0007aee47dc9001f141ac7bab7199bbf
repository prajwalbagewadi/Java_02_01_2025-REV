package pkg;

import java.lang.*;
import java.util.*;
import java.io.*;

public class OneDArray {
	public static void main(String[] args) {
		//syntax
		// data-type arr_name[] = new data-type(size)
		int arr[] = new int[5];  //dynamic
		int arr1[] = {1,2,3,4,5}; //static
		
		for(int i=0;i<5;i++) {
			System.out.print(arr1[i]+" ");
		}
	}
}
